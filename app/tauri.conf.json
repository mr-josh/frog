{
  "$schema": "../www/node_modules/@tauri-apps/cli/schema.json",
  "build": {
    "beforeBuildCommand": {
      "script": "npm run build",
      "cwd": "../www"
    },
    "beforeDevCommand": {
      "script": "npm run dev",
      "cwd": "../www"
    },
    "devPath": "http://localhost:5173",
    "distDir": "../www"
  },
  "package": {
    "productName": "frog",
    "version": "0.1.0"
  },
  "tauri": {
    "allowlist": {
      "fs": {
        "exists": true,
        "readFile": true,
        "scope": [
          "$DOCUMENT/frog.txt"
        ]
      },
      "window": {
        "close": true,
        "setIgnoreCursorEvents": true,
        "setPosition": true
      }
    },
    "systemTray": {
      "iconPath": "icons/icon.png",
      "iconAsTemplate": true
    },
    "bundle": {
      "active": true,
      "category": "DeveloperTool",
      "copyright": "",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "com.tauri.dev",
      "longDescription": "",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [],
      "shortDescription": "",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "label": "frog-0",
        "fullscreen": false,
        "width": 200,
        "height": 200,
        "resizable": false,
        "transparent": true,
        "title": "FROG",
        "alwaysOnTop": true,
        "decorations": false,
        "skipTaskbar": true
      }
    ]
  }
}